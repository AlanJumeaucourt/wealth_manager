{
    "compilerOptions": {
        "jsx": "react-native",
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "module": "esnext",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "noEmit": true,
        "target": "esnext"
    },
    "exclude": [
        "node_modules"
    ]
}